<Window x:Class="PLWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        
        xmlns:local="clr-namespace:PLWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="467" Width="734">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="195*"/>
            <ColumnDefinition Width="47*"/>
        </Grid.ColumnDefinitions>
        <Menu x:Name="menu" Grid.Column="1" Height="435" Margin="0,0,6,0" VerticalAlignment="Top">
            <Button x:Name="button" Content="Main" Width="125" Height="41" Click="button_Click"/>
            <Button x:Name="button_Copy" Content="Button" Width="125" Height="41"/>
            <Button x:Name="button_Copy1" Content="Button" Width="125" Height="41"/>
            <Button x:Name="button_Copy2" Content="Button" Width="125" Height="41" Click="button_Copy2_Click"/>
            <Button x:Name="button_Copy3" Content="Setting" Width="125" Height="41"/>
        </Menu>
        <Frame x:Name="MainFrame"   Margin="0,0,0,1" NavigationUIVisibility="Hidden"/>

    </Grid>
</Window>
